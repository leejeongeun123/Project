<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns ="http://javafx.com/javafx/8"
fx:controller="basic.control.ViewController"
	prefHeight="180" prefWidth="500">
	
	<children>
		<Label layoutX="11" layoutY="9" text="ListView" />
		<ListView fx:id="listView" layoutX="10" layoutY="30"
			prefHeight="100" prefWidth="100" />
		<Label layoutX="125" layoutY="9" text="TableView" />
		<TableView fx:id="tableView" layoutX="120" layoutY="30"
			prefHeight="100" prefWidth="290">
			<columns>
				<TableColumn prefWidth="100" text="스마트폰" />
				<TableColumn prefWidth="100" text="이미지" />
			</columns>
		</TableView>
		<Label layoutX="425" layoutY="9" text="ImageView" />
		<ImageView fx:id="imageView" fitHeight="100" fitWidth="60"
			layoutX="430" layoutY="30" preserveRatio="true" />
		<Button layoutX="190" layoutY="145" text="확인" />
		<Button layoutX="260" layoutY="145" text="취소" />

	</children>

</AnchorPane>

